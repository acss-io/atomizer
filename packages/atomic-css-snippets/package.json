{
    "name": "atomic-css-snippets",
    "displayName": "Atomic CSS Snippets",
    "description": "Provides quick suggestions for Atomic CSS class names",
    "version": "1.0.11",
    "license": "MIT",
    "publisher": "acss-io",
    "devDependencies": {
        "atomizer": "^3.29.0",
        "vsce": "^2.10.0"
    },
    "repository": {
        "directory": "packages/atomic-css-snippets",
        "type": "git",
        "url": "git://github.com/acss-io/atomizer.git"
    },
    "scripts": {
        "build": "node scripts/generate-snippets.js > snippets.json",
        "deploy": "vsce publish --no-dependencies",
        "package": "vsce package --no-dependencies"
    },
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "Atomic CSS",
        "ACSS",
        "Atomizer",
        "CSS",
        "Snippets",
        "VSCode",
        "Yahoo"
    ],
    "icon": "acss-io.png",
    "contributes": {
        "snippets": [
            {
                "language": "html",
                "path": "snippets.json"
            },
            {
                "language": "javascript",
                "path": "snippets.json"
            },
            {
                "language": "javascriptreact",
                "path": "snippets.json"
            },
            {
                "language": "marko",
                "path": "snippets.json"
            },
            {
                "language": "php",
                "path": "snippets.json"
            },
            {
                "language": "svelte",
                "path": "snippets.json"
            },
            {
                "language": "typescript",
                "path": "snippets.json"
            },
            {
                "language": "typescriptreact",
                "path": "snippets.json"
            }
        ]
    }
}
