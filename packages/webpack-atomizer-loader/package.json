{
    "name": "webpack-atomizer-loader",
    "version": "1.1.0",
    "description": "Webpack loader for compiling atomic css",
    "main": "index.js",
    "scripts": {
        "example": "cd example && webpack && open index.html && cd ..",
        "build": "tsc",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "prepublish": "npm run build",
        "pretest": "npm run build",
        "test": "mocha tests"
    },
    "keywords": [
        "atomizer",
        "atomic",
        "css",
        "loader",
        "webpack"
    ],
    "author": "tom76kimo <tom76kimo@gmail.com> ",
    "license": "MIT",
    "dependencies": {
        "atomizer": "^3.9.2",
        "cssnano": "^5.0.7",
        "loader-utils": "^2.0.0",
        "postcss": "^8.3.6"
    },
    "repository": "git@github.com:acss-io/webpack-atomizer-loader.git",
    "devDependencies": {
        "@babel/cli": "^7.14.8",
        "@babel/core": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-react": "^7.14.5",
        "@types/core-js": "^2.5.0",
        "@types/node": "^16.4.14",
        "autoprefixer": "^10.3.1",
        "babel-loader": "^8.2.2",
        "chai": "^4.3.4",
        "create-react-class": "^15.6.3",
        "mocha": "^9.0.3",
        "prettier": "^2.3.2",
        "react": "^16.6.3",
        "react-dom": "^16.6.3",
        "typescript": "^4.3.5",
        "webpack": "^5.50.0",
        "webpack-cli": "^4.7.2"
    },
    "engines": {
        "node": ">=12"
    }
}
